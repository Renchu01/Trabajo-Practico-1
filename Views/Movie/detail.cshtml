@model Movie


<h2> @Model.Name</h2>

<p> Duracion: @Model.Minutes</p>

<p> @Model.Review</p>


<form asp-action="Delete"
asp-route-code="@Model.Code"
onsubmit="return confirm('Estas seguro que deseas eliminar esta pelicula? ')">
    <button type="submit" class="btn btn-danger"> Eliminar</button>
</form>